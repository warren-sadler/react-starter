{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime", { "regenerator": true }],
        [
            "babel-plugin-transform-imports",
            {
                "@material-ui/core": {
                    "transform": "@material-ui/core/${member}",
                    "preventFullImport": true
                }
            }
        ]
    ]
}
